<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.phynos.framework.dao.mapper.ext.UserRoleMapperWrapper">

    <select id="getRolesByUserId" resultMap="com.phynos.framework.dao.mapper.UserRoleMapper.BaseResultMap">
